        <activity android:name=".SeeAllCategories" />
        <activity android:name=".otp_login" />
        <activity android:name=".Sign_up_activity" />
        <activity android:name=".SearchUniversal" />
        <activity android:name=".SignUp" />
        <activity android:name=".Login_1" />
        <activity android:name=".LogoLauncher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>