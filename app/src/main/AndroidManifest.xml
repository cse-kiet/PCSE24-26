<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodyhomeSS">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.CALL_PHONE"
        android:required="true" />
    <uses-permission
        android:name="android.permission.READ_CONTACTS"
        android:required="true" />
    <uses-permission
        android:name="android.permission.SEND_SMS"
        android:required="true" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodyHome">
        <activity android:name=".Payment_Activity"/>
        <activity android:name=".AllProduct" />
        <activity android:name=".YourMenu"/>
        <activity android:name=".OrderDetails" />
        <activity android:name=".Individual_Product" />
        <activity
            android:name=".Termandcondition"
